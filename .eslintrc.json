{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off"  ,
        "quotes": [2, "single", { "avoidEscape": true }],
        "camelcase": "off",
        "import/extensions": "off"
           
    }
}
